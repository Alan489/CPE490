import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		new Main();
	}
	
	public Main()
	{
		out D = new out("Main");
		D.info("\nAlan Decowski // CPE-490 // Final Project\n2020 Fall - Offline multi-person document editor");
		pause(500);
		D.info("Server-Side");
		pause(1000);
		D.warn("Terminal interface only.");
		pause(1000);
		D.info("Please enter the port number you wish to bind to.");
		System.out.print("> ");
		Scanner in = new Scanner(System.in);
		
		String port = in.nextLine();
		int pt = Integer.parseInt(port);
		
		in.close();
	}

	public static void pause(long ms)
	{
		long end = System.currentTimeMillis() + ms;
		while (System.currentTimeMillis() < end);
	}
	
}
